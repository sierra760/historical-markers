{
  "expo": {
    "name": "Arizona Historical Markers",
    "slug": "historicalmarkers-az",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/az/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/az/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#c9b9af"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "buildNumber": "1",
      "bundleIdentifier": "com.sierraburkhart.historicalmarkers.arizona",
      "supportsTablet": true,
      "infoPlist": {
        "CFBundleDisplayName": "AZ History"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/az/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.sierraburkhart.historicalmarkers.arizona",
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE"
      ]
    },
    "web": {
      "favicon": "./assets/az/favicon.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "If you allow Arizona Historical Markers to use your location while the app is running, your location will be used to sort the list of historical markers so that the markers nearest you are shown first.",
          "locationAlwaysAndWhenInUsePermission": "If you allow Arizona Historical Markers to use your location while the app is running, your location will be used to sort the list of historical markers so that the markers nearest you are shown first.",
          "isIosBackgroundLocationEnabled": false,
          "isAndroidBackgroundLocationEnabled": false
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "c43c4e86-ba93-4538-bd24-1707649fb3c4"
      }
    }
  }
}
