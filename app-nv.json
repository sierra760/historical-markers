{
  "expo": {
    "name": "Nevada Historical Markers",
    "slug": "historicalmarkers-nv",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/nv/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/nv/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#004b83"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "buildNumber": "1",
      "bundleIdentifier": "com.sierraburkhart.historicalmarkers.nevada",
      "supportsTablet": true,
      "infoPlist": {
        "CFBundleDisplayName": "NV History",
        "NSAppTransportSecurity": { "NSAllowsArbitraryLoads": true }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/nv/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.sierraburkhart.historicalmarkers.nevada"
    },
    "web": {
      "favicon": "./assets/nv/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "2c6a48df-b189-478a-9294-aa17cf4ce31d"
      }
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "If you allow Nevada Historical Markers to use your location while the app is running, your location will be used to sort the list of historical markers so that the markers nearest you are shown first.",
          "locationAlwaysAndWhenInUsePermission": "If you allow Nevada Historical Markers to use your location while the app is running, your location will be used to sort the list of historical markers so that the markers nearest you are shown first.",
          "isIosBackgroundLocationEnabled": false,
          "isAndroidBackgroundLocationEnabled": false
        }
      ]
    ]
  }
}
